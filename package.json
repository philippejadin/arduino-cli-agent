{
  "name": "arduino-cli-agent",
  "version": "0.1.0",
  "description": "A simple server to allow local compilation and upload of sketches, using arduino-cli called from python",
  "main": "start.js",
  "scripts": {
    "build-linux": "pkg -t node8-linux-x64 --out-path dist/linux start.js",
    "build-windows": "pkg -t node8-win --out-path dist/windows start.js",
    "build-mac": "pkg -t node8-macos --out-path dist/mac start.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "decompress": "^4.2.0",
    "decompress-tarbz2": "^4.1.1",
    "decompress-unzip": "^4.0.1",
    "express": "^4.16.4",
    "mkdirp": "^0.5.1",
    "node-wget": "^0.4.3"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philippejadin/arduino-cli-agent.git"
  },
  "author": "",
  "license": "GPL",
  "bugs": {
    "url": "https://github.com/philippejadin/arduino-cli-agent/issues"
  },
  "homepage": "https://github.com/philippejadin/arduino-cli-agent#readme"
}
